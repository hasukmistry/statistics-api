{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.11",
        "fakerphp/faker": "^1.19",
        "nesbot/carbon": "^2.57",
        "phpunit/phpunit": "^9.5",
        "sensio/framework-extra-bundle": "^6.2",
        "symfony/browser-kit": "6.0.*",
        "symfony/console": "6.0.*",
        "symfony/dotenv": "6.0.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "6.0.*",
        "symfony/maker-bundle": "^1.38",
        "symfony/monolog-bundle": "^3.7",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/property-access": "6.0.*",
        "symfony/proxy-manager-bridge": "6.0.*",
        "symfony/runtime": "6.0.*",
        "symfony/serializer": "6.0.*",
        "symfony/yaml": "6.0.*"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts",
            "@app.preparation"
        ],
        "post-update-cmd": [
            "@auto-scripts",
            "@app.preparation"
        ],
        "db-preparation": [
            "php bin/console doctrine:database:create -q --if-not-exists",
            "php bin/console doctrine:migrations:migrate -q --allow-no-migration --no-interaction",
            "php bin/console doctrine:schema:validate"
        ],
        "app.preparation": [
            "@db-preparation",
            "php bin/console c:c --no-warmup"
        ],
        "app.preparation.test": [
            "@db-preparation --env=test",
            "php bin/console c:c --no-warmup --env=test"
        ],
        "db_fixtures": [
            "php bin/console doctrine:fixtures:load --no-interaction"
        ],
        "tests": [
            "php -d pcov.enabled=1 vendor/bin/phpunit"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.0.*"
        }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4"
    }
}
